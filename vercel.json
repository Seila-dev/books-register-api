{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json", 
        "use": "@vercel/static-build"
      },
      {
        "src": "api/products.ts", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/products.ts"
      }
    ]
  }
  